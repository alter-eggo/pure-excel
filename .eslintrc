{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "object-curly-spacing": [2, "always"],
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "space-before-function-paren": "off",
    "max-len": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
